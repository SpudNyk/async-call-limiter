{
    "name": "async-wrappers",
    "gitRevision": "master",
    "readme": "./typedoc.md",
    "exclude": ["src/index.ts"],
    "theme": "minimal",
    "categorizeByGroup": true,
    "categoryOrder": [
        "Wrapper",
        "Debounce",
        "Retry",
        "Throttle",
