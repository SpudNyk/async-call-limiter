{
    "name": "async-wrappers",
    "version": "2.0.0",
    "description": "A set of wrapper functions to perform debouncing, throttling, retrying etc.",
    "keywords": [
        "debounce",
        "async",
        "throttle",
        "promise",
        "async-functions",
        "typescript"
    ],
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "exports": {
        ".": {
            "node": "cjs/index.js"
        }
    },
    "types": "types/index.d.ts",
    "homepage": "https://spudnyk.github.io/async-call-limiter",
    "repository": "https://github.com/SpudNyk/async-call-limiter.git",
    "license": "MIT",
    "sideEffects": false,
    "scripts": {
        "dist:clean": "dist-package clean",
        "dist:package": "dist-package package",
        "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
        "build:cjs": "cross-env BABEL_ENV=cjs babel src -d dist/cjs --extensions \".ts,.tsx\"",
        "build:esm": "cross-env BABEL_ENV=esm babel src -d dist/esm --extensions \".ts,.tsx\"",
        "build:es": "cross-env BABEL_ENV=es babel src -d dist/es --extensions \".ts,.tsx\"",
        "build": "npm-run-all dist:clean build:cjs build:esm build:es build:types dist:package",
        "docs": "typedoc",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.4",
        "@babel/plugin-transform-modules-commonjs": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-typescript": "^7.8.3",
        "@sinonjs/fake-timers": "^6.0.0",
        "@types/jest": "^25.1.2",
        "@types/lolex": "^5.1.0",
        "@types/node": "^13.7.1",
        "cross-env": "^7.0.0",
        "dist-package": "^0.2.1",
        "eslint": "^6.8.0",
        "fs-jetpack": "^2.2.3",
        "jest": "^25.1.0",
        "jsdoc-to-markdown": "^5.0.3",
        "npm-run-all": "^4.1.5",
        "typedoc": "^0.16.9",
        "typescript": "^3.7.5",
        "yargs": "^15.1.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "dependencies": {
        "docs": "^0.3.2-canary.0"
    }
}
